<body class="home">
<header>
  Online advertising car
</header>
<h2>.</h2>
<%= devise_error_messages! %>
    <% flash.each do |key, value| %>
       <div class="flash <%= key %>"><%= value %></div>
<% end %>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class = "container">
    <div class="field">
    <%= f.email_field :email, autofocus: true, placeholder: "Email", class:"credentials", autocomplete: "email" %>
    </div>
    </br>
    <div class="field">
    <%= f.password_field :password, placeholder: "Password", class:"credentials", autocomplete: "current-password" %>
    </div>
    </br>
  <div class="actions">
    <%= f.submit "Log in", class:"submit" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</body>
